<h3>Replace head with:</h3>
<div class="row small-up-1 medium-up-2 large-up-2">
  <% @heads.each do |head| %>
  <div class="column head_swap_option tiki_select">
    <%= image_tag(head.thumbnail_path, id: head.name, 'data-height' => head.height, 'data-obj' => head.object_path) %><br/>
    <%= head.name %>
  </div>
  <% end %>
</div>
