
<h3>Begin by selecting a base totem:</h3>
<div class="row small-up-1 medium-up-2 large-up-2">
  <% @bases.each do |base| %>
  <div class="column base_option tiki_select">
    <%= image_tag(base.thumbnail_path, id: base.name, 'data-height' => base.height, 'data-obj' => base.object_path) %><br/>
    <%= base.name %>
  </div>
  <% end %>
</div>
